<!--
When using the CLion IDE, which files it tries to look in
to for scanning and indexing can go a bit wild (for some
unknown reason). Notably it seems to want to include
/Library/Developer/CommandLineTools stuff and stuff in
$USER_HOME. If I try and exclude these, excluding $USER_HOME
is too restrictive and masks the project. Hence I have had to
resort to this ugly solution that might need to be
copied (in part) into .idea/misc.xml
-->

<?xml version="1.0" encoding="UTF-8"?>
<project version="4">
  <component name="CMakeWorkspace" PROJECT_DIR="$PROJECT_DIR$" />
  <component name="CidrRootsConfiguration">
    <sourceRoots>
      <file path="$PROJECT_DIR$/src" />
    </sourceRoots>
    <excludeRoots>
      <file path="/Library/Developer/CommandLineTools/SDKs/MacOSX13.1.sdk/System/Library/Frameworks" />
      <file path="/Library/Developer/CommandLineTools/SDKs/MacOSX13.1.sdk/System/Library/Frameworks/_AppIntents_AppKit.framework/Modules" />
      <file path="/Library/Developer/CommandLineTools/SDKs/MacOSX13.1.sdk/usr/include" />
      <file path="$PROJECT_DIR$/build" />
      <file path="$PROJECT_DIR$/docs" />
      <file path="$PROJECT_DIR$/site" />
      <file path="/usr/local/Cellar/pcre2/10.42/include" />
      <file path="/usr/local/include" />
      <file path="/usr/local/opt/gettext/include" />
